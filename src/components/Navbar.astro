---
const links = [
  { href: '/', text: 'Home' },
  { href: '/#about', text: 'About' },
  { href: '/#projects', text: 'Projects' },
  { href: '/#contact', text: 'Contact' },
];
---

<nav class=" p-4 flex flex-row justify-between">
  <div>Logo</div>
  <div class="flex gap-6">
      {links.map(link => (
        <a href={link.href} class=" no-underline hover:underline">{link.text}</a>
      ))}
  </div>
</nav>
